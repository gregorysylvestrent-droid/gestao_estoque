+  isOpen,
+  onClose,
+  onSave
+}) => {
+  const [formData, setFormData] = useState<Omit<Mechanic, 'id' | 'productivity' | 'currentWorkOrders'>>({
+    name: '',
+    specialty: '',
+    shift: 'manha',
+    status: 'disponivel'
+  });
+
+  const [isLoading, setIsLoading] = useState(false);
+
+  if (!isOpen) return null;
+
+  const handleSubmit = async (e: React.FormEvent) => {
+    e.preventDefault();
+    setIsLoading(true);
+    try {
+      await onSave(formData);
+      onClose();
+      // Reset form
+      setFormData({
+        name: '',
+        specialty: '',
+        shift: 'manha',
+        status: 'disponivel'
