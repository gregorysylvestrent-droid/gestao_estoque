+    );
+
+    expect(result[0].status).toBe('aguardando');
+    expect(result[0].type).toBe('corretiva');
+    expect(result[0].priority).toBe('normal');
+    expect(result[0].services[0].category).toBe('outros');
+    expect(result[0].parts[0].qtyRequested).toBe(5);
+  });
