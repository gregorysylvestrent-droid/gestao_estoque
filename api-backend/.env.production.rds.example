PORT=3001
NODE_ENV=production

# RDS PostgreSQL
DB_HOST=your-rds-endpoint.us-east-1.rds.amazonaws.com
DB_PORT=5432
DB_NAME=logiwms_db
DB_USER=admin_logiwms
DB_PASSWORD=replace-with-strong-password
DB_SSL=true
DB_SSL_REJECT_UNAUTHORIZED=false

# Security
JWT_SECRET=replace-with-very-strong-secret-min-32-chars
JWT_EXPIRES_IN=8h

# Frontend origin served by EC2 + nginx (change to your domain later)
CORS_ORIGIN=http://EC2_PUBLIC_IP

# Optional
FLEET_SYNC_ALLOWED_HOSTS=cubogpm-frota.nortesistech.com
